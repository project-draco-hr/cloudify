{
  if (params.length != 1) {
    throw new IllegalStateException("command " + name + " requires one param of type 'long'");
  }
  try {
    Long.parseLong(params[0].toString());
  }
 catch (  NumberFormatException e) {
    throw new IllegalStateException("parameter type mismatch. can't convert " + params[0].toString() + " to 'long'",e);
  }
}
