{
  if (request == null) {
    throw new IllegalArgumentException("request may not be null");
  }
  InvokeServiceCommandResponse result=null;
  try {
    final String invokeCommandUrl=getFormattedUrl(versionedDeploymentControllerUrl,INVOKE_SERVICE_COMMAND_URL_FORMAT,applicationName,serviceName,beanName);
    result=executor.postObject(invokeCommandUrl,request,new TypeReference<Response<InvokeServiceCommandResponse>>(){
    }
);
  }
 catch (  final RestClientException e) {
  }
  return result;
}
