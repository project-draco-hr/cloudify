{
  try {
    final String procName=this.sigar.getProcExe(pid).getName();
    for (    String shellName : SHELL_PROCESS_NAMES) {
      if (procName.indexOf(shellName) >= 0) {
        logger.warning("The monitored process is " + procName + ", which may be a console process. "+ "This is usually a configuration problem. USM Statistics will be collected for this process, "+ "and not for the child process it probably has. Are you missing a Start Detector?");
        return;
      }
    }
  }
 catch (  SigarException e) {
    logger.log(Level.SEVERE,"While checking if process is a console, failed to read the process name for process: " + pid,e);
  }
}
