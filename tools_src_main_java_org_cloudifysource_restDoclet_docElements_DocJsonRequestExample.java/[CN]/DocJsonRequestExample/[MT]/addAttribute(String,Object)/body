{
  String value=attrValue.toString().trim();
  String shortAttrName=getShortName(attrName);
  if (shortAttrName.equals(RestDocConstants.JSON_REQUEST_EXAMPLE_REQUEST_PARAMS))   requestJsonBody=value;
 else   if (shortAttrName.equals(RestDocConstants.JSON_REQUEST_EXAMPLE_COMMENTS))   comments=value;
  attributes.put(shortAttrName,value);
}
