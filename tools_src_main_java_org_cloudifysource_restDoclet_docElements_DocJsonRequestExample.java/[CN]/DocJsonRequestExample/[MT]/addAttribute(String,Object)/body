{
  String value=attrValue.toString().trim();
  String shortAttrName=getShortName(attrName);
  if (RestDocConstants.JSON_REQUEST_EXAMPLE_REQUEST_PARAMS.equals(shortAttrName))   requestJsonBody=value;
 else   if (RestDocConstants.JSON_REQUEST_EXAMPLE_COMMENTS.equals(shortAttrName))   comments=value;
  super.addAttribute(shortAttrName,value);
}
