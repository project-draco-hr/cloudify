{
  if (input == null) {
    return "";
  }
  Pattern pattern=Pattern.compile(regex,regexFlags);
  Matcher matcher=pattern.matcher(input);
  int beginIndex=0;
  int endIndex=0;
  if (matcher.find()) {
    beginIndex=matcher.start(0);
    endIndex=matcher.end(0);
  }
  return input.substring(beginIndex,endIndex);
}
