{
  final String url=SERVICE_CONTROLLER_URL + "applications/" + applicationName+ "/services/"+ serviceName+ "/instances/"+ instanceId+ "/beans/"+ beanName+ "/invoke";
  @SuppressWarnings("unchecked") Map<String,String> resultMap;
  try {
    resultMap=(Map<String,String>)client.post(url,buildCustomCommandParams(commandName,paramsMap));
  }
 catch (  final ErrorStatusException e) {
    throw new CLIStatusException(e,e.getReasonCode(),e.getArgs());
  }
catch (  final RestException e) {
    throw new CLIException(e);
  }
  final InvocationResult invocationResult=InvocationResult.createInvocationResult(resultMap);
  return invocationResult;
}
