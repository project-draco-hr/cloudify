{
  String result=null;
  try {
    result=client.postFile(SERVICE_CONTROLLER_URL + CLOUD_CONTROLLER_URL + "deploy?"+ "applicationName="+ applicationName,packedFile);
  }
 catch (  final ErrorStatusException e) {
    throw new CLIStatusException(e,e.getReasonCode(),e.getArgs());
  }
catch (  final RestException e) {
    throw new CLIException(e);
  }
  return result;
}
