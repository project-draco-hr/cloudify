{
  MaximumInstancesStatisticsConfig config=new MaximumInstancesStatisticsConfig();
  InstancesStatisticsConfig instanceStatistics=(InstancesStatisticsConfig)configFactory.createStatisticsConfig(config);
  Assert.assertTrue("InstanceStatistics is expected to be instanceof MaximumInstancesStatisticsConfig",instanceStatistics instanceof org.openspaces.admin.pu.statistics.MaximumInstancesStatisticsConfig);
}
