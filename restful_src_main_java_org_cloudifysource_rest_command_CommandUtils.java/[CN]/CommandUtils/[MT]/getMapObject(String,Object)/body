{
  Map<?,?> map=(Map<?,?>)mapObject;
  for (  Map.Entry<?,?> entry : map.entrySet()) {
    if (entry.getKey().toString().equals(key)) {
      return entry.getValue();
    }
  }
  throw new RuntimeException("Error while accessing map of type " + map.getClass().getSimpleName() + ". Map does not contain a value for the key: "+ key);
}
