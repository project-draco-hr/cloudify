{
  try {
    File templatesFile=new File(TEMPLATES_FILE_PATH);
    ServiceReader.getCloudTemplatesFromFile(templatesFile);
  }
 catch (  Exception e) {
    Assert.fail("failed to read templates from file " + TEMPLATES_FILE_PATH + " error message is "+ e.getMessage());
  }
  try {
    File templateFile=new File(TEMPLATE_FILE_PATH);
    ServiceReader.getCloudTemplatesFromFile(templateFile);
  }
 catch (  Exception e) {
    Assert.fail("failed to read template from file " + TEMPLATE_FILE_PATH + " error message is "+ e.getMessage());
  }
}
