{
  final CompilerConfiguration cc=new CompilerConfiguration();
  final ImportCustomizer ic=new ImportCustomizer();
  ic.addStarImports(com.gigaspaces.cloudify.dsl.Service.class.getPackage().getName(),UserInterface.class.getPackage().getName(),com.gigaspaces.cloudify.dsl.context.Service.class.getPackage().getName());
  ic.addImports(com.gigaspaces.cloudify.dsl.utils.ServiceUtils.class.getName());
  cc.addCompilationCustomizers(ic);
  cc.setScriptBaseClass(baseClassName);
  cc.setClasspathList(extraJarFileNames);
  return cc;
}
