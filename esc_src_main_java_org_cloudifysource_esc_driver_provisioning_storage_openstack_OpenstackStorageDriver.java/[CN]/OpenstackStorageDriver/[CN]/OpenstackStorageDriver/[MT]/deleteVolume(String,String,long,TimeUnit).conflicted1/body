{
  Optional<? extends VolumeApi> volumeApi=getVolumeApi(region);
  if (!volumeApi.isPresent()) {
    throw new StorageProvisioningException("Failed to delete volume " + volumeId + ", Openstack API is not "+ "initialized.");
  }
  throw new StorageProvisioningException("Failed to delete volume " + volumeId + ", Openstack API is not "+ "initialized.");
}
