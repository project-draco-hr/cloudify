{
  final WebResource webResource=this.getWebResource();
  if (logger.isLoggable(Level.FINER)) {
    logger.log(Level.FINER,"Request=deleteServer: " + serverId);
  }
  webResource.path("servers/" + serverId).type(MediaType.APPLICATION_JSON_TYPE).accept(MediaType.APPLICATION_JSON).header("X-Auth-Token",this.getTokenId()).delete();
}
