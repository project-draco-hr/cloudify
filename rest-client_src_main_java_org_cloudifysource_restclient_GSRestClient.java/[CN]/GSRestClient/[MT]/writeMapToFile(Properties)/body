{
  File tempFile=null;
  FileWriter writer=null;
  try {
    tempFile=File.createTempFile("uploadTemp",".tmp");
    writer=new FileWriter(tempFile,true);
    if (props != null) {
      props.store(writer,"");
    }
    tempFile.deleteOnExit();
  }
  finally {
    if (writer != null) {
      try {
        writer.close();
      }
 catch (      final IOException e) {
      }
    }
  }
  return tempFile;
}
