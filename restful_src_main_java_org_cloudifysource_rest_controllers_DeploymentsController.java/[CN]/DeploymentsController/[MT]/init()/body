{
  gigaSpace=restConfig.getGigaSpace();
  permissionEvaluator=restConfig.getPermissionEvaluator();
  logger.warning("***** starting DeolpymentsController, injecting rest temp folder to uploadrepo: " + restConfig.getRestTempFolder().getAbsolutePath());
  repo.init(restConfig.getRestTempFolder());
