{
  AverageInstancesStatisticsConfig config=new AverageInstancesStatisticsConfig();
  InstancesStatisticsConfig instanceStatistics=configFactory.createInstanceStatisticsConfig(config);
  Assert.assertTrue("InstanceStatistics is expected to be instanceof AverageInstancesStatisticsConfig",instanceStatistics instanceof org.openspaces.admin.pu.statistics.AverageInstancesStatisticsConfig);
}
