{
  TimeWindowStatisticsConfig config=new MaximumTimeWindowStatisticsConfig();
  org.openspaces.admin.pu.statistics.TimeWindowStatisticsConfig timeWindowStatistics=configFactory.createTimeWindowStatisticsConfig(config);
  assertDefaultTimeWindowValues(timeWindowStatistics);
  Assert.assertTrue("InstanceStatistics is expected to be instanceof MaximumTimeWindowStatisticsConfig",timeWindowStatistics instanceof org.openspaces.admin.pu.statistics.MaximumTimeWindowStatisticsConfig);
}
