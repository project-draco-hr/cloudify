{
  final String templateName=service.getCompute().getTemplate();
  if (this.cloud != null) {
    if (templateName != null) {
      final CloudTemplate template=cloud.getTemplates().get(templateName);
      if (template == null) {
        throw new RestErrorException(CloudifyErrorMessages.MISSING_TEMPLATE.getName(),templateName);
      }
    }
  }
}
