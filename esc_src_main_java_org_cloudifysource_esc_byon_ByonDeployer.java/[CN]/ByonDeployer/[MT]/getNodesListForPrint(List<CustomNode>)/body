{
  StringBuilder nodesStr=new StringBuilder();
  boolean first=true;
  for (  CustomNode node : nodesToPrint) {
    if (first) {
      nodesStr.append("CustomNode[" + node.toShortString() + "]");
      first=false;
    }
 else {
      nodesStr.append(", CustomNode[" + node.toShortString() + "]");
    }
  }
  return nodesStr.toString();
}
