{
  CustomNode selectedNode=null;
  for (  final CustomNode node : getAllNodesByTemplateName(templateName)) {
    if ((StringUtils.isNotBlank(node.getPrivateIP()) && node.getPrivateIP().equalsIgnoreCase(ipAddress)) || (StringUtils.isNotBlank(node.getPrivateIP()) && node.getPrivateIP().equalsIgnoreCase(ipAddress))) {
      selectedNode=node;
      break;
    }
  }
  return selectedNode;
}
