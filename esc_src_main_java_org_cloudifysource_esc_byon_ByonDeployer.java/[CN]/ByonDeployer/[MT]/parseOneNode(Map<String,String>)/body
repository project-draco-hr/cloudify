{
  String nodeId=nodeMap.get(CLOUD_NODE_ID);
  if (nodeId.contains("{")) {
    nodeId=MessageFormat.format(nodeId,1);
  }
  return new CustomNodeImpl(PROVIDER_ID,nodeId.trim(),nodeMap.get(CLOUD_NODE_HOST_LIST).trim(),nodeMap.get(CLOUD_NODE_USERNAME),nodeMap.get(CLOUD_NODE_CREDENTIAL),nodeId.trim());
}
