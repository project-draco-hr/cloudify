{
  final String response=this.getWebResource().path("ports").queryParam("device_id",serverId).queryParam("network_id",networkId).type(MediaType.APPLICATION_JSON_TYPE).accept(MediaType.APPLICATION_JSON).header("X-Auth-Token",this.getTokenId()).get(String.class);
  final List<Port> ports=JsonUtils.unwrapRootToList(Port.class,response);
  if (ports == null || ports.isEmpty()) {
    return null;
  }
  return ports.get(0);
}
