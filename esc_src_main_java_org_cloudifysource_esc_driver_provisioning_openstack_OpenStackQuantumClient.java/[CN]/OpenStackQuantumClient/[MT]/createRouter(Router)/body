{
  final String jsonRequest=JsonUtils.toJson(request);
  final String response=this.getWebResource().path("routers").type(MediaType.APPLICATION_JSON_TYPE).accept(MediaType.APPLICATION_JSON).header("X-Auth-Token",this.getTokenId()).post(String.class,jsonRequest);
  final Router router=JsonUtils.unwrapRootToObject(Router.class,response);
  return router;
}
