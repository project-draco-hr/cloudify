{
  final String response=this.getWebResource().path("subnets").queryParam("network_id",networkId).type(MediaType.APPLICATION_JSON_TYPE).accept(MediaType.APPLICATION_JSON).header("X-Auth-Token",this.getTokenId()).get(String.class);
  final SubnetsResponse subnets=JsonUtils.mapJsonToObject(SubnetsResponse.class,response);
  return subnets.getSubnets();
}
