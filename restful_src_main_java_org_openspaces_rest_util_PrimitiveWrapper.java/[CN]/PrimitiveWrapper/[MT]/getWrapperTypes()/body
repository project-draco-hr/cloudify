{
  HashSet<Class<?>> ret=new HashSet<Class<?>>();
  ret.add(Boolean.class);
  ret.add(Character.class);
  ret.add(Byte.class);
  ret.add(Short.class);
  ret.add(Integer.class);
  ret.add(Long.class);
  ret.add(Float.class);
  ret.add(Double.class);
  ret.add(Void.class);
  ret.add(String.class);
  return ret;
}
