{
  String str="@" + name;
  if (attributes != null && attributes.size() > 0) {
    str+="{";
    for (    Entry<String,Object> entry : attributes.entrySet()) {
      str+=entry.getKey().toString() + "=" + entry.getValue().toString()+ ", ";
    }
    str=str.substring(0,str.lastIndexOf(","));
    str+="}";
  }
  return str;
}
