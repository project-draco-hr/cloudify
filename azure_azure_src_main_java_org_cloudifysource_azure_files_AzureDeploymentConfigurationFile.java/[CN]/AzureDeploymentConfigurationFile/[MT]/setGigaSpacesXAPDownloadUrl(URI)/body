{
  String urlEncodedGigaspacesURL=null;
  try {
    urlEncodedGigaspacesURL=URLEncoder.encode(gigaSpacesUrl.toString(),"UTF-8");
  }
 catch (  UnsupportedEncodingException e) {
    throw new IllegalStateException("UTF-8 must exist",e);
  }
  getEditor().setNodeAttribute("/ServiceConfiguration/Role/ConfigurationSettings/Setting[@name='GigaSpaces.XAP.DownloadUrl']","value",urlEncodedGigaspacesURL);
}
