{
  TimeWindowStatisticsConfig config=new PercentileTimeWindowStatisticsConfig();
  org.openspaces.admin.pu.statistics.TimeWindowStatisticsConfig timeWindowStatistics=configFactory.createTimeWindowStatistics(config);
  assertDefaultTimeWindowValues(timeWindowStatistics);
  Assert.assertTrue("InstanceStatistics is expected to be instanceof PercentileTimeWindowStatisticsConfig",timeWindowStatistics instanceof org.openspaces.admin.pu.statistics.PercentileTimeWindowStatisticsConfig);
}
