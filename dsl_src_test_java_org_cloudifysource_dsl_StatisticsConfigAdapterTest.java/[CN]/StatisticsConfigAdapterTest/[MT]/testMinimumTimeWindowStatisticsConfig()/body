{
  TimeWindowStatisticsConfig config=new MinimumTimeWindowStatisticsConfig();
  org.openspaces.admin.pu.statistics.TimeWindowStatisticsConfig timeWindowStatistics=configFactory.createTimeWindowStatistics(config);
  assertDefaultTimeWindowValues(timeWindowStatistics);
  Assert.assertTrue("InstanceStatistics is expected to be instanceof MinimumTimeWindowStatisticsConfig",timeWindowStatistics instanceof org.openspaces.admin.pu.statistics.MinimumTimeWindowStatisticsConfig);
}
