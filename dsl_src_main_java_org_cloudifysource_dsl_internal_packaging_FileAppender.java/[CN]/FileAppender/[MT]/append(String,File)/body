{
  writer.append("//" + comment + " Generated By "+ FileAppender.class.getName()+ " On : "+ new Date(System.currentTimeMillis()));
  writer.append(LINE_SEPARATOR);
  writer.append(LINE_SEPARATOR);
  writer.append(FileUtils.readFileToString(fileToMerge));
  writer.append(LINE_SEPARATOR);
  writer.append(LINE_SEPARATOR);
  return this;
}
