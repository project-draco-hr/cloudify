{
  try {
    this.driver.assign(ip,instanceID,DEFAULT_NETWORK_OPERATION_TIMEOUT_MILLIS,TimeUnit.MILLISECONDS);
  }
 catch (  final NetworkProvisioningException e) {
    logger.warning("failed to assign IP " + ip + " to instance with ID "+ instanceID+ ". Error was "+ e.getMessage());
    throw new RemoteNetworkOperationException("failed to assign IP " + ip + " to instance with ID "+ instanceID+ " : "+ e.getMessage(),e);
  }
}
