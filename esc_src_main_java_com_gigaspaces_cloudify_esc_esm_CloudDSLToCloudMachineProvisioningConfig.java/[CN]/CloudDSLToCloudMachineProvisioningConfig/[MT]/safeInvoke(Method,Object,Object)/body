{
  try {
    Object result=method.invoke(object,args);
    return result;
  }
 catch (  IllegalArgumentException e) {
    logger.log(Level.WARNING,"Cloud Machine Provisioning Config was unable to set " + method.getName().substring(2) + ". Using default value");
    e.printStackTrace();
  }
catch (  IllegalAccessException e) {
    logger.log(Level.WARNING,"Cloud Machine Provisioning Config was unable to set " + method.getName().substring(2) + ". Using default value");
  }
catch (  InvocationTargetException e) {
    logger.log(Level.WARNING,"Cloud Machine Provisioning Config was unable to set " + method.getName().substring(2) + ". Using default value");
    e.printStackTrace();
  }
  return null;
}
