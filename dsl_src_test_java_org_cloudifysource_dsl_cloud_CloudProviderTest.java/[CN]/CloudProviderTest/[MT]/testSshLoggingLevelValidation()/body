{
  try {
    ServiceReader.readCloud(new File(INVALID_SSH_LOG_LEVEL_PATH));
    assertTrue("The ssh logging level is invalid yet no error was thrown",false);
  }
 catch (  final Throwable e) {
    assertTrue("The ssh logging level is invalid yet no relevant error was thrown. Error was: " + e.getMessage(),e.getMessage().contains("INFO, FINE, FINER, FINEST, WARNING, DEBUG"));
  }
}
