{
  try {
    ServiceReader.readCloud(new File(WRONG_PROVIDER_PATH));
    assertTrue("The provider name is invalid yet no error was thrown",false);
  }
 catch (  final Throwable e) {
    assertTrue("The provider name is invalid yet no relevant error was thrown. Error was: " + e.getMessage(),e.getMessage().contains("Provider") && e.getMessage().contains("is not supported"));
  }
}
