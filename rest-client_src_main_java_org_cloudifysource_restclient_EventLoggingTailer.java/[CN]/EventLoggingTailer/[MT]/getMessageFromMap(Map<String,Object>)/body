{
  String cleanEventText=(lifecycleEventMap.get(EventLogConstants.getEventTextKey())).toString().split(" - ")[1];
  String outputMessage='[' + lifecycleEventMap.get(EventLogConstants.getMachineHostNameKey()).toString() + '/'+ lifecycleEventMap.get(EventLogConstants.getMachineHostAddressKey())+ "] "+ cleanEventText;
  return outputMessage;
}
