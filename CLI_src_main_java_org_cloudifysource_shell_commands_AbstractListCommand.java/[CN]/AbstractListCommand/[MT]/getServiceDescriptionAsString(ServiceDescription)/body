{
  StringBuilder sb=new StringBuilder();
  String absolutePUName=ServiceUtils.getAbsolutePUName(serviceDescription.getApplicationName(),serviceDescription.getServiceName());
  sb.append(ShellUtils.getBoldMessage(absolutePUName) + "  " + serviceDescription.getServiceState());
  sb.append(" (" + serviceDescription.getInstanceCount() + "/"+ serviceDescription.getPlannedInstances()+ ")");
  return sb.toString();
}
