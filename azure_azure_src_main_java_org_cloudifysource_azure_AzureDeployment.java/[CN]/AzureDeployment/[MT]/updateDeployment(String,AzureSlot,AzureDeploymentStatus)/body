{
  if (!(status == AzureDeploymentStatus.Running || status == AzureDeploymentStatus.Suspended)) {
    throw new IllegalArgumentException("status must be either Running or Suspended");
  }
  executeAzureConfig(argument(UPDATE_DEPLOYMENT_FLAG),argument(HOSTED_SERVICE_FLAG,hostedServiceName),argument(SLOT_FLAG,deploymentSlot.getSlot()),argument(STATUS_FLAG,status.getStatus()));
}
