{
  if (this.context != currentContext) {
    return;
  }
  logger.warning("Reseting JClouds Deployer");
  final Set<Module> wiring=new HashSet<Module>();
  this.context.close();
  this.context=ContextBuilder.newBuilder(provider).credentials(account,key).modules(wiring).overrides(overrides).buildView(ComputeServiceContext.class);
}
