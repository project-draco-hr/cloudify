{
  final CompilerConfiguration cc=new CompilerConfiguration();
  final ImportCustomizer ic=new ImportCustomizer();
  ic.addStarImports(org.cloudifysource.dsl.Service.class.getPackage().getName(),UserInterface.class.getPackage().getName(),org.cloudifysource.dsl.internal.context.ServiceImpl.class.getPackage().getName());
  ic.addImports(org.cloudifysource.dsl.utils.ServiceUtils.class.getName());
  cc.addCompilationCustomizers(ic);
  cc.setScriptBaseClass(baseClassName);
  cc.setClasspathList(extraJarFileNames);
  return cc;
}
