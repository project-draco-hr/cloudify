{
  final CloudTemplate template=this.cloud.getTemplates().get(this.cloudTemplateName);
  final ExactZonesConfig zones=new ExactZonesConfigurer().addZones(config.getGridServiceAgentZones().getZones()).addZone(CLOUD_ZONE_PREFIX + md.getLocationId()).create();
  final InstallationDetails details=Utils.createInstallationDetails(md,cloud,template,zones,lookupLocatorsString,this.originalESMAdmin,false,null);
  logger.info("Created new Installation Details: " + details);
  return details;
}
