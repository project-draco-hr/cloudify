{
  String eventText=(map.get(EventLogConstants.getEventTextKey())).toString();
  int delimiterIndex=eventText.indexOf('-');
  String cleanEventText=eventText.substring(delimiterIndex);
  String outputMessage='[' + map.get(EventLogConstants.getMachineHostNameKey()).toString() + '/'+ map.get(EventLogConstants.getMachineHostAddressKey())+ "] "+ cleanEventText;
  return outputMessage;
}
