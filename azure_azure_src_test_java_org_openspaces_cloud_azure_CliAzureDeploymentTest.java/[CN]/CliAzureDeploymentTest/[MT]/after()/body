{
  if (cliExecutablePath != null) {
    log("Destroying deployment");
    List<List<String>> commands=new ArrayList<List<String>>();
    List<String> azureTeardownApplication=Arrays.asList("azure:teardown-app","-azure-svc",AZURE_HOSTED_SERVICE,"-timeout",String.valueOf(TIMEOUT_IN_MINUTES),"-progress",String.valueOf(POLLING_INTERVAL_IN_MINUTES));
    commands.add(azureTeardownApplication);
    runCliCommands(cliExecutablePath,commands,isDebugMode);
    commands.clear();
  }
}
