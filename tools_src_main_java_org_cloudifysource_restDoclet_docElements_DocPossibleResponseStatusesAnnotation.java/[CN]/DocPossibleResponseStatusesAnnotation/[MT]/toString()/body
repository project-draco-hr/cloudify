{
  String str="@" + RestDocConstants.POSSIBLE_RESPONSE_STATUSES_ANNOTATION + "[";
  str+="codes: {";
  for (int i=0; i < codes.length; i++) {
    if (i != 0)     str+=", ";
    str+=codes[i];
  }
  str+="}, descriptions: {";
  for (int i=0; i < descriptions.length; i++) {
    if (i != 0)     str+=", ";
    str+=descriptions[i];
  }
  str+="}";
  return str + "]";
}
